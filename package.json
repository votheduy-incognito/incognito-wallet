  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:reset": "react-native start --reset-cache",
    "android:reload": "adb shell input keyevent 82",
    "flow": "flow",
    "android": "react-native run-android --variant=debug",
    "androidStaging": "react-native run-android --variant=releaseStaging",
    "androidNext": "react-native run-android --variant=releaseNext",
    "android:clean": "cd android && rm app/src/main/assets/index.android.bundle && ./gradlew clean",
    "buildDebug": "npx jetify && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle && cd android && ./gradlew app:assembleDebug",
    "ios:device": "react-native run-ios --device=\"iPhone\"",
    "ios": "react-native run-ios --simulator=\"iPhone 6s\"",
    "buildIosDebug": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "buildRelease": "npx jetify && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle && cd android && ./gradlew app:assembleRelease",
    "buildStaging": "npx jetify && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle && cd android && ./gradlew app:assembleReleaseStaging",
    "buildNext": "npx jetify && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle && cd android && ./gradlew app:assembleReleaseNext",
    "buildNext2": "npx jetify && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle && cd android && ./gradlew app:assembleReleasNext2",
    "buildNext3": "npx jetify && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle && cd android && ./gradlew app:assembleReleasNext3",
    "test": "jest",
    "lint": "eslint .",
    "lint-staged": "lint-staged",
    "ios:clean": "yarn cache clean && rm -rf ~/Library/Developer/Xcode/DerivedData",
    "postinstall": "./node_modules/.bin/rn-nodeify --install crypto,stream,vm --hack && npx jetify",
    "devtools": "react-devtools",
    "deployIOS:prod": "appcenter codepush release-react -a 0xSiris/Incognito -t \"50.0\" -d Production -x",
    "deployIOS:staging": "appcenter codepush release-react -a 0xSiris/Incognito -t \"50.0\" -d Staging",
    "deployIOS:next": "appcenter codepush release-react -a 0xSiris/Incognito -t \"50.0\" -d NextRelease",
    "deployIOS:next2": "appcenter codepush release-react -a 0xSiris/Incognito -t \"50.0\" -d NextRelease2",
    "deployIOS:next3": "appcenter codepush release-react -a 0xSiris/Incognito -t \"50.0\" -d NextRelease3",
    "deployAndroid:staging": "appcenter codepush release-react -a 0xSiris/Incognito-Android -t \"4.0.0\" -d Staging",
    "deployAndroid:prod": "appcenter codepush release-react -a 0xSiris/Incognito-Android -t \"4.0.0\" -d Production -x",
    "deployAndroid:next": "appcenter codepush release-react -a 0xSiris/Incognito-Android -t \"4.0.0\" -d NextRelease",
    "deployAndroid:next2": "appcenter codepush release-react -a 0xSiris/Incognito-Android -t \"4.0.0\" -d NextRelease2",
    "deployAndroid:next3": "appcenter codepush release-react -a 0xSiris/Incognito-Android -t \"4.0.0\" -d NextRelease3",
    "deploy:dev": "yarn deployIOS:dev && yarn deployAndroid:dev",
    "deploy:staging": "yarn deployIOS:staging && yarn deployAndroid:staging",
    "deploy:prod": "yarn deployIOS:prod && yarn deployAndroid:prod",
    "deploy:next": "yarn deployIOS:next && yarn deployAndroid:next",
    "deploy:next2": "yarn deployIOS:next2 && yarn deployAndroid:next2",
    "deploy:next3": "yarn deployIOS:next3 && yarn deployAndroid:next3"
